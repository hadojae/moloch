  background: linear-gradient(to bottom,  #162758 0%,#445b9a 50%,#445b9a 100%) /* W3C */

.fixedheader
  position: fixed
  right: 0px
  left: 0px
  background-color: #0f0 ! important

.fixedrow
  position: fixed
  right: 0px
  left: 0px
  z-index: 0

#sessions_processing

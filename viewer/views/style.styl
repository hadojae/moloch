.fixedsearch
  position: fixed
  top: 0px
  right: 0px
  left: 0px
  background-color: #0f0 ! important;
  background: #162758; /* Old browsers */
  background: -moz-linear-gradient(top,  #162758 0%, #445b9a 50%, #445b9a 100%) /* FF3.6+ */
